  "description": "JSON Graph viewer - part of jsonGraph Visual JSON data flow inspector - paste JSON or fetch from API endpoints and visualize as interactive node graphs.",
  "version": "0.0.1",
  "license": "MIT",
  "publisher": "RatneshDesign",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "keywords": [
    "json",
    "json viewer",
    "json visualizer",
    "json graph",
    "tree view",
    "node graph",
    "api",
    "data visualization"
  ],
